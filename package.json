{
  "name": "yt-dlp-webui",
  "version": "1.1.0",
  "description": "A terrible webUI for yt-dlp, all-in-one solution.",
  "scripts": {
    "dev": "nodemon dist/main.js",
    "start": "node dist/main.js",
    "watch": "tsc --build -w",
    "build": "vite build",
    "build-server": "tsc --build",
    "build-all": "tsc --build && npm run build && npm run fetch",
    "clean": "tsc --build --clean",
    "clean-all": "rm -r dist",
    "fe": "vite",
    "fetch-dev": "./fetch-yt-dlp.sh && mv yt-dlp ./server/core",
    "fetch": "./fetch-yt-dlp.sh && mv yt-dlp ./dist/core"
  },
  "author": "marcobaobao",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@koa/cors": "^3.3.0",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "@reduxjs/toolkit": "^1.8.1",
    "koa": "^2.13.4",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "mime-types": "^2.1.35",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "rxjs": "^7.4.0",
    "socket.io": "^4.3.2",
    "socket.io-client": "^4.3.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.0.2",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.31",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^1.3.2",
    "buffer": "^6.0.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "typescript": "^4.6.4",
    "vite": "^2.9.10"
  }
}